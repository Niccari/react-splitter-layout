{
  "name": "react-splitter-layout",
  "version": "4.0.0",
  "description": "A simple split layout for React and modern browsers",
  "keywords": [
    "react",
    "layout",
    "split",
    "splitter"
  ],
  "author": "Yang Liu <hi@zesik.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zesik/react-splitter-layout"
  },
  "bugs": {
    "url": "https://github.com/zesik/react-splitter-layout/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.2",
    "css-loader": "^7.0.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jsdom": "^13.2.0",
    "raf": "^3.4.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "peerDependencies": {
    "prop-types": "^15.5.0",
    "react": ">=15.5.0 <20"
  },
  "scripts": {
    "prepublish": "webpack --progress",
    "clean": "rm -rf ./lib",
    "build": "webpack --progress",
    "lint": "eslint '@(src|test|example)/**/*.js?'",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "example": "cd example && webpack-dev-server"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ]
  }
}
