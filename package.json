{
  "name": "react-splitter-layout",
  "version": "4.0.1",
  "description": "A simple split layout for React and modern browsers",
  "keywords": [
    "react",
    "layout",
    "split",
    "splitter"
  ],
  "author": "Yang Liu <hi@zesik.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zesik/react-splitter-layout"
  },
  "bugs": {
    "url": "https://github.com/zesik/react-splitter-layout/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "coveralls-next": "^6.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.4.0",
    "raf": "^3.4.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.0",
    "react": ">=15.5.0 <20"
  },
  "scripts": {
    "prepublish": "webpack --progress",
    "clean": "rm -rf ./lib",
    "build": "webpack --progress",
    "lint": "eslint '@(src|test|example)/**/*.js?'",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "example": "cd example && webpack-dev-server"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "./test/setup.js"
    ]
  }
}
